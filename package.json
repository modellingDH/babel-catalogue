{
  "name": "babel-catalogue",
  "version": "1.0.1",
  "description": "A 3D animated book component built with React Three Fiber for visualizing AI cognitive states and data retrieval processes",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "components",
    "stores",
    "hooks",
    "types",
    "index.ts",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "next dev --webpack",
    "build": "npm run build:lib && npm run build:next",
    "build:lib": "tsc --project tsconfig.lib.json",
    "build:next": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepublishOnly": "npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modellingDH/babel-catalogue.git"
  },
  "keywords": [
    "react",
    "three",
    "r3f",
    "react-three-fiber",
    "3d",
    "animation",
    "book",
    "ai",
    "visualization",
    "cognitive-states"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modellingDH/babel-catalogue/issues"
  },
  "homepage": "https://github.com/modellingDH/babel-catalogue#readme",
  "peerDependencies": {
    "@react-three/drei": "^10.0.0",
    "@react-three/fiber": "^9.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "three": "^0.160.0",
    "zustand": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/three": "^0.160.0",
    "leva": "^0.10.1",
    "next": "^16.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0",
    "three-stdlib": "^2.36.1",
    "typescript": "^5.0.0",
    "zustand": "^5.0.9"
  }
}
